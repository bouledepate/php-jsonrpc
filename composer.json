{
  "name": "bouledepate/php-jsonrpc",
  "type": "library",
  "description": "Lightweight JSON-RPC Middleware for PHP Applications",
  "keywords": [
    "json-rpc",
    "php",
    "middleware",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Semyon Shmik",
      "email": "promtheus815@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "guzzlehttp/psr7": "^2.7",
    "psr/container": "^1.1 || ^2.0",
    "psr/http-factory": "^1.1",
    "psr/http-message": "^1.1 || ^2.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.4",
    "slim/psr7": "*",
    "slim/slim": "*",
    "php-di/php-di": "*",
    "nyholm/psr7": "*"
  },
  "autoload": {
    "psr-4": {
      "Bouledepate\\JsonRpc\\": "JsonRpc/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bouledepate\\JsonRpc\\Tests\\": "tests/JsonRpc/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
